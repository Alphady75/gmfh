<div class="wt-dashboard-msg-search-list-wrap single-user p-0">
	{% if conversation.user1.id == app.user.id %}
		<div class="d-flex p-0 m-0 no-hover">
			{% if conversation.user2.compte == 'ENTREPRISE' %}
					<img src="{% if conversation.user2.logo %}{{ vich_uploader_asset(conversation.user2, 'logoFile' ) | imagine_filter('largeavatar') }}{% else %}{{ asset('img/banner.png') | imagine_filter('largeavatar') }}{% endif %}" class="rounded-circle" alt="{{ conversation.user2.compte == 'ENTREPRISE' ? conversation.user2.societe ? : conversation.user2 }}">
				{% else %}
					<img src="{% if conversation.user2.photo %}{{ vich_uploader_asset(conversation.user2, 'photoFile' ) | imagine_filter('largeavatar') }}{% else %}{{ asset('img/avatar.jpg') | imagine_filter('largeavatar') }}{% endif %}" class="rounded-circle" alt="{{ conversation.user2.compte == 'ENTREPRISE' ? conversation.user2.societe ? : conversation.user2 }}">
				{% endif %}
			<div class="msg-user-info-text no-hover">
				<div class="msg-user-name">
					{{ conversation.user2.compte == 'ENTREPRISE' ? conversation.user2.societe : conversation.user2 }}
				</div>
				<div class="msg-user-discription">
					{{ conversation.user2.compte == 'ENTREPRISE' ? conversation.user2.secteuractivite : conversation.user2.qualification }}
				</div>
			</div>
		</div>
	{% else %}
		<div class="d-flex p-0 m-0 no-hover">
			{% if conversation.user1.compte == 'ENTREPRISE' %}
				<img src="{% if conversation.user1.logo %}{{ vich_uploader_asset(conversation.user1, 'logoFile' ) | imagine_filter('largeavatar') }}{% else %}{{ asset('img/banner.png') | imagine_filter('largeavatar') }}{% endif %}" class="rounded-circle" alt="{{ conversation.user1.compte == 'ENTREPRISE' ? conversation.user1.societe ? : conversation.user1 }}">
			{% else %}
				<img src="{% if conversation.user1.photo %}{{ vich_uploader_asset(conversation.user1, 'photoFile' ) | imagine_filter('largeavatar') }}{% else %}{{ asset('img/avatar.jpg') | imagine_filter('largeavatar') }}{% endif %}" class="rounded-circle" alt="{{ conversation.user1.compte == 'ENTREPRISE' ? conversation.user1.societe ? : conversation.user1 }}">
			{% endif %}
			<div class="msg-user-info-text no-hover">
				<div class="msg-user-name">
					{{ conversation.user1.compte == 'ENTREPRISE' ? conversation.user1.societe : conversation.user1 }}
				</div>
				<div class="msg-user-discription">
					{{ conversation.user1.compte == 'ENTREPRISE' ? conversation.user1.secteuractivite : conversation.user1.qualification }}
				</div>
			</div>
		</div>
	{% endif %}
</div>
