										<div class="twm-log-reg-logo">
											<h1 class="log-reg-form-title">{% block titre %}{% endblock %}</h1>
										</div>
									</div>

									<p class="text-center">{% block text %}{% endblock %}</p>

									{% block links %}{% endblock %}

									{% include "partials/base/_flash.html.twig" %}

									{% block body %}{% endblock %}

									{% block footer %}{% endblock %}

									<div class="text-center mt-3">
										<a href="{{ path('accueil') }}">Retourner sur accueil &longrightarrow;</a>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
