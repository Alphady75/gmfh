  <script>
    document.getElementById("go-back").addEventListener("click", () => {
      history.back();
    });
  </script>

  {% block javascripts %}
  {{ encore_entry_script_tags('app') }}
  {{ encore_entry_script_tags('collection') }}
  {{ encore_entry_script_tags('favoris') }}
