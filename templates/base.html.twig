  <!-- IMAGE POPUP -->
  <script src="{{ asset('themes/base/js/bootstrap-select.min.js') }}"></script>
  <!-- Form js -->
  <script src="{{ asset('themes/base/js/dropzone.js') }}"></script>
  <!-- IMAGE UPLOAD  -->
  <script src="{{ asset('themes/base/js/jquery.scrollbar.js') }}"></script>
  <!-- scroller -->
