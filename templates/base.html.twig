</head>

<body data-turbo="false" class="{% block bodybg %}{% endblock %}">

  <!-- LOADING AREA START ===== -->
  <div class="loading-area">
    <div class="loading-box"></div>
    <div class="loading-pic">
      <div class="wrapper">
        <div class="cssload-loader"></div>
      </div>
    </div>
  </div>
  <!-- LOADING AREA  END ====== -->

  <div class="page-wraper">

    <!-- HEADER START -->
    {% block header %}
    {% include "partials/base/_header_two.html.twig" %}
    {% endblock %}
    <!-- HEADER END -->

    <!-- CONTENT START -->
    <div class="page-content">

      {% include "partials/base/_flash.html.twig" %}

      {% block body %}{% endblock %}

    </div>
    <!-- CONTENT END -->

    <!-- FOOTER START -->
    {% block footer %}
    {% include "partials/base/_footer.html.twig" %}
    {% endblock %}
    <!-- FOOTER END -->

    <!-- FOOTER START -->
    {% block mobilenav %}
    {% include "partials/base/_mobile_nav.html.twig" %}
    {% endblock %}
    <!-- FOOTER END -->

    <!-- BUTTON TOP START -->
    <button class="scroltop">
      <span class="fa fa-angle-up  relative" id="btn-vibrate"></span>
    </button>

  </div>

