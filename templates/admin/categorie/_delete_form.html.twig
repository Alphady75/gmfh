<form method="post" class="d-inline" action="{{ path('admin_categorie_delete', {'id': categorie.id}) }}" onsubmit="return confirm('Êtes-vous sûr de bien vouloir supprimer cet élément?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ categorie.id) }}">
    <button class="btn text-danger"><i class="fa fa-trash-alt"></i></button>
</form>
