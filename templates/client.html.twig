<!DOCTYPE html>
<html lang="fr">

<head>

  <!-- META -->
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="keywords" content="" />
  <meta name="author" content="" />
  <meta name="robots" content="" />
  <meta name="description" content="" />

  <!-- FAVICONS ICON -->
  <link rel="icon" href="{{ asset('img/logo/logo-g/favicon.png') }}" sizes="32x32" type="image/x-icon" />
  <link rel="shortcut icon" type="image/x-icon" href="{{ asset('img/logo/logo-g/favicon.png') }}" sizes="32x32" />

  <!-- PAGE TITLE HERE -->
  <title>{% block title %}{{ appName }}{% endblock %}</title>

  <!-- MOBILE SPECIFIC -->
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <link rel="stylesheet" type="text/css" href="{{ asset('themes/base/css/bootstrap.min.css') }}">
  <!-- BOOTSTRAP STYLE SHEET -->
  <link rel="stylesheet" type="text/css" href="{{ asset('themes/base/css/font-awesome.min.css') }}">
  <!-- FONTAWESOME STYLE SHEET -->
  <link rel="stylesheet" type="text/css" href="{{ asset('themes/base/css/feather.css') }}"><!-- FEATHER ICON SHEET -->
  <link rel="stylesheet" type="text/css" href="{{ asset('themes/base/css/owl.carousel.min.css') }}">
  <!-- OWL CAROUSEL STYLE SHEET -->
  <link rel="stylesheet" type="text/css" href="{{ asset('themes/base/css/magnific-popup.min.css') }}">
  <!-- MAGNIFIC POPUP STYLE SHEET -->
  <link rel="stylesheet" type="text/css" href="{{ asset('themes/base/css/lc_lightbox.css') }}">
  <!-- Lc light box popup -->
  <link rel="stylesheet" type="text/css" href="{{ asset('themes/base/css/bootstrap-select.min.css') }}">
  <!-- BOOTSTRAP SLECT BOX STYLE SHEET  -->
  <link rel="stylesheet" type="text/css" href="{{ asset('themes/base/css/dataTables.bootstrap5.min.css') }}">
  <!-- DATA table STYLE SHEET  -->
  <link rel="stylesheet" type="text/css" href="{{ asset('themes/base/css/select.bootstrap5.min.css') }}">
  <!-- DASHBOARD select bootstrap  STYLE SHEET  -->
  <link rel="stylesheet" type="text/css" href="{{ asset('themes/base/css/dropzone.css') }}">
  <!-- DROPZONE STYLE SHEET -->
  <link rel="stylesheet" type="text/css" href="{{ asset('themes/base/css/scrollbar.css') }}">
  <!-- CUSTOM SCROLL BAR STYLE SHEET -->
  <link rel="stylesheet" type="text/css" href="{{ asset('themes/base/css/datepicker.css') }}">
  <!-- DATEPICKER STYLE SHEET -->
  <link rel="stylesheet" type="text/css" href="{{ asset('themes/base/css/flaticon.css') }}"> <!-- Flaticon -->
  <link rel="stylesheet" type="text/css" href="{{ asset('themes/base/css/swiper-bundle.min.css') }}">
  <!-- Swiper Slider -->
  <link rel="stylesheet" type="text/css" href="{{ asset('themes/base/css/style.css') }}"><!-- MAIN STYLE SHEET -->

  <!-- THEME COLOR CHANGE STYLE SHEET -->
  <link rel="stylesheet" class="skin" type="text/css" href="{{ asset('themes/base/css/skins-type/skin-6.css') }}">
  {% block stylesheets %}
  {{ encore_entry_link_tags('app') }}
  {{ encore_entry_link_tags('responsive') }}
  {% endblock %}

</head>

<body data-turbo="false" class="espace client">
  <!-- LOADING AREA START ===== -->
  <div class="loading-area">
    <div class="loading-box"></div>
    <div class="loading-pic">
      <div class="wrapper">
        <div class="cssload-loader"></div>
      </div>
    </div>
  </div>
  <!-- LOADING AREA  END ====== -->

  <div class="page-wraper">

    <!-- HEADER START -->
    {% block header %}
    {% include "partials/base/_header_two.html.twig" %}
    {% endblock %}
    <!-- HEADER END -->


    <!-- CONTENT START -->
    <div class="page-content">

      <!-- OUR BLOG START -->
      <div class="section-full pt-3  p-b90 site-bg-white">

        <div class="container">
          <div class="row">

            <div class="col-xl-3 col-lg-4 col-md-12 rightSidebar m-b30">

              {% include "partials/client/_sidebar.html.twig" %}

            </div>

            <div class="col-xl-9 col-lg-8 col-md-12 m-b30">

              {% include "partials/base/_flash.html.twig" %}

              <div class="twm-right-section-panel site-bg-gray">
                {% block titreVisible %}
                <h2 class="page-titre mt-0 mb-3 text-dark text-start">{% block titre %}{% endblock %}</h2>
                {% endblock %}

                {% block body %}{% endblock %}

              </div>

            </div>

          </div>
        </div>
      </div>

    </div>
    <!-- CONTENT END -->

    <!-- FOOTER START -->
    {% block footer %}
    {% include "partials/base/_footer.html.twig" %}
    {% endblock %}
    <!-- FOOTER END -->

    <!-- BUTTON TOP START -->
    <button class="scroltop"><span class="fa fa-angle-up  relative" id="btn-vibrate"></span></button>

  </div>


  <!-- JAVASCRIPT  FILES ========================================= -->
  <!-- JAVASCRIPT  FILES ========================================= -->
  <script src="{{ asset('themes/base/js/jquery-3.6.0.min.js') }}"></script><!-- JQUERY.MIN JS -->
  <script src="{{ asset('themes/base/js/popper.min.js') }}"></script><!-- POPPER.MIN JS -->
  <script src="{{ asset('themes/base/js/bootstrap.min.js') }}"></script><!-- BOOTSTRAP.MIN JS -->
  <script src="{{ asset('themes/base/js/magnific-popup.min.js') }}"></script><!-- MAGNIFIC-POPUP JS -->
  <script src="{{ asset('themes/base/js/waypoints.min.js') }}"></script><!-- WAYPOINTS JS -->
  <script src="{{ asset('themes/base/js/counterup.min.js') }}"></script><!-- COUNTERUP JS -->
  <script src="{{ asset('themes/base/js/waypoints-sticky.min.js') }}"></script><!-- STICKY HEADER -->
  <script src="{{ asset('themes/base/js/isotope.pkgd.min.js') }}"></script><!-- MASONRY  -->
  <script src="{{ asset('themes/base/js/imagesloaded.pkgd.min.js') }}"></script><!-- MASONRY  -->
  <script src="{{ asset('themes/base/js/owl.carousel.min.js') }}"></script><!-- OWL  SLIDER  -->
  <script src="{{ asset('themes/base/js/theia-sticky-sidebar.js') }}"></script><!-- STICKY SIDEBAR  -->
  <script src="{{ asset('themes/base/js/lc_lightbox.lite.js') }}"></script><!-- IMAGE POPUP -->
  <script src="{{ asset('themes/base/js/bootstrap-select.min.js') }}"></script><!-- Form js -->
  <script src="{{ asset('themes/base/js/dropzone.js') }}"></script><!-- IMAGE UPLOAD  -->
  <script src="{{ asset('themes/base/js/jquery.scrollbar.js') }}"></script><!-- scroller -->
  <script src="{{ asset('themes/base/js/bootstrap-datepicker.js') }}"></script><!-- scroller -->
  <script src="{{ asset('themes/base/js/jquery.dataTables.min.js') }}"></script><!-- Datatable -->
  <script src="{{ asset('themes/base/js/dataTables.bootstrap5.min.js') }}"></script><!-- Datatable -->
  <script src="{{ asset('themes/base/js/chart.js') }}"></script><!-- Chart -->
  <script src="{{ asset('themes/base/js/bootstrap-slider.min.js') }}"></script><!-- Price range slider -->
  <script src="{{ asset('themes/base/js/swiper-bundle.min.js') }}"></script><!-- Swiper JS -->
  <script src="{{ asset('themes/base/js/custom.js') }}"></script><!-- CUSTOM FUCTIONS  -->

  {% block javascripts %}
  {{ encore_entry_script_tags('app') }}
  {{ encore_entry_script_tags('collection') }}
  {{ encore_entry_script_tags('select2') }}
  {{ encore_entry_script_tags('offresecteurs') }}
  {{ encore_entry_script_tags('mce') }}
  {{ encore_entry_script_tags('loadbtn') }}
  {{ encore_entry_script_tags('desablebtn') }}
  {{ encore_entry_script_tags('hideflash') }}
  {% endblock %}
</body>

</html>